<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<div align="center" >
			<g:VerticalPanel >
				<g:Label  text="Login" />
				<g:TextBox ui:field="loginBox"  />
				<g:Label text="Password" />
				<g:TextBox ui:field="passwordBox"  />
				<g:HorizontalPanel>
					<g:Button ui:field="buttonSubmit" text="Submit" />
					<g:CheckBox ui:field="myCheckBox"  />
					<g:Label ui:field="myLabel" text="Remember me" />
				</g:HorizontalPanel>
				<g:Label ui:field="completionLabel1"  />
				<g:Label ui:field="completionLabel2" />
			</g:VerticalPanel>
		</div>
	</g:HTMLPanel>
</ui:UiBinder> 